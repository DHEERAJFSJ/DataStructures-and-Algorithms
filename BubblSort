
//BubblSort
import java.util.*;
public class Demo
{
public static void main(String args[])
{
int[] arr={90,80,45,76,23,16,79,34};
bubbleSort(arr);
System.out.println(Arrays.toString(arr));
}
static void bubbleSort(int[] arr)
{
for(int i=0;i<arr.length;i++)
{
int temp=i;
for(int j=0;j<arr.length-1-i;j++)
{
if(arr[j]>arr[j+1])
{
temp=arr[j];
arr[j]=arr[j+1];
arr[j+1]=temp;
}
}
}
}
}
