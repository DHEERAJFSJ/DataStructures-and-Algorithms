//Reversing an array 
import java.util.*;
public class Demo
{
  public static void main(String args[])
  {
    int[] arr={10,20,30,40,50,60,70};
    reverse(arr);
    System.out.prinltn(Arrays.toString(arr));
  }
  static void reverse(int[] arr)
  {
    int i=0,j=arr.length-1;
    while(i<j)
      {
      int temp=arr[i];
      arr[i]=arr[j];
      arr[j]=temp;
      }
  }
}
