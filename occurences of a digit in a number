//number of occurences of a digit 
package com.iw.reg;
import java.util.*;
import java.lang.*;
public class Demo
{
	public static void main(String[] args)
	{
		//program to count the occurences of a number 
		int number=12345673,count=0;
		while(number>0)
		{
			if(number%10==3)
			{
				count++;
			}
			number/=10;
		}
		System.out.println(count);
	}
	}
